    </b:data>
    <b:block name="serp-list">
        <b:mod name="mod1" val="val1"/>
        <b:mix>
            <b:block name="bla">
                <b:mod name="mod3" val="val3"/>
            </b:block>
            <b:elem name="alb" block="bla2"/>
